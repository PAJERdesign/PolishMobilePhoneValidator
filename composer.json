{
    "name": "pajerdesign/polish-mobile-phone-validator",
    "type": "symfony-bundle",
    "description": ".",
    "keywords": ["symfony", "validation", "phone number", "mobile phone number"],
    "homepage": "http://pajerdesign.pl",
    "license": "MIT",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Robert Pajer",
            "email": "robert.pajer@pajerdesign.pl"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "~6.0|~7.0",
        "symfony/dependency-injection": "~6.0|~7.0",
        "symfony/validator": "~6.0|~7.0",
        "ext-mbstring": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "PAJERdesign\\PolishMobilePhoneValidator\\": "src/" }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}